{
  "name": "@spartanbio/schedio-tokens",
  "version": "2.1.0",
  "description": "Design tokens used by @spartanbio/schedio",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "github:spartanbio/schedio-tokens",
  "author": "Spartan Bioscience Inc.",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "lib/**/*",
    "scss/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "run-s clean build:**:*",
    "build:theo": "gulp",
    "build:ase": "node ./scripts/ase-encode.js",
    "build:docs": "gulp docs",
    "build:format:style": "stylelint ./{docs,dist}/**/*.{scss,css} --fix",
    "build:format:js": "eslint ./{docs,dist}/**/*.{js,ts} --fix --no-error-on-unmatched-pattern",
    "build:index": "node scripts/build-index.js",
    "build:ts": "tsc",
    "clean": "rimraf ./dist ./lib",
    "docs": "run-s build:docs build:format:js build:format:style",
    "lint:js": "eslint .",
    "lint:style": "stylelint ./**/*.{scss,css}",
    "prepublishOnly": "yarn build",
    "semantic-release": "semantic-release",
    "watch": "gulp watch"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@spartanbio/eslint-config": "^1.1.1",
    "@spartanbio/stylelint-config-scss": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "ase-utils": "^0.1.1",
    "browser-sync": "^2.26.7",
    "chalk": "^4.0.0",
    "eslint": "^7.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "fs-extra": "^9.0.0",
    "gulp": "^4.0.2",
    "gulp-load-plugins": "^2.0.3",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-restart": "^0.1.1",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-theo": "^2.0.1",
    "husky": "^5.0.9",
    "lint-staged": "^10.2.6",
    "lodash": "^4.17.15",
    "ms": "^2.1.2",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.8",
    "stylelint": "^13.5.0",
    "theo": "^8.1.5",
    "tinycolor2": "^1.4.1",
    "typescript": "^4.0.2"
  }
}
